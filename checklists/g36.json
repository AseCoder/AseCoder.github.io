{
	"title": "Bonanza G36 Checklists",
	"copyright": "Source: Model G36 Bonanza Pilot's Operating Handbook<br>© 2013 Beechcraft Corporation<br>Modified",
	"checklists": [
		[
			"BEFORE ENGINE STARTING",
			[
				"Subpanel Switches -- OFF, BEACON ON",
				"Throttle -- CLOSED",
				"Propeller -- HIGH RPM",
				"Mixture -- FULL RICH",
				"Cowl Flaps -- OPEN",
				"Flaps -- UP",
				"Battery 1 & 2, Alternator 1 & 2 -- ON",
				"Alerts -- CHECK & CONSIDERED",
				"Fuel Selector Valve -- SELECT FULLER TANK"
			]
		],
		[
			"ENGINE STARTING (BATTERY)",
			"COLD STARTS",
			[
				"Throttle -- FULL OPEN",
				"Propeller -- HIGH RPM",
				"Mixture -- FULL RICH",
				"Aux Fuel Pump -- HI UNTIL FUEL FLOW PEAKS THEN OFF",
				"Throttle -- CLOSE, THEN OPEN APPROXIMATELY 1/2 INCH",
				"Magneto/Start Switch -- START",
				"Throttle -- 1000 to 1200 RPM AFTER START"
			],
			"FLOODED ENGINE",
			[
				"Mixture -- CUT OFF",
				"Propeller -- HIGH RPM",
				"Throttle -- 1/2 OPEN",
				"Magneto/Start Switch -- START",
				"As Engine Starts:",
				[
					{
						"indent": 1
					},
					"Throttle -- IDLE",
					"Mixture -- FULL RICH"
				]
			],
			"HOT STARTS",
			[
				"Mixture -- CUT OFF",
				"Propeller -- HIGH RPM",
				"Aux Fuel Pump -- HI FOR 30-60 SECONDS, THEN OFF",
				"Mixture -- FULL RICH",
				"Throttle -- FULL OPEN",
				"Aux Fuel Pump -- HI UNTIL FUEL FLOW PEAKS THEN OFF",
				"Throttle -- CLOSE, THEN OPEN APPROXIMATELY 1/2 INCH",
				"Magneto/Start Switch -- START",
				"Aux Fuel Pump (if required) -- HI",
				"Aux Fuel Pump -- OFF"
			]
		],
		[
			"BEFORE TAXI",
			[
				"Throttle -- 1000 to 1200 RPM",
				"Avionics Master -- ON",
				"Lights -- AS REQUIRED",
				"Avionics -- CHECK & SET",
				[
					{
						"ul": true
					},
					"Altimeters",
					"FLIGHT PLAN",
					"XPDR",
					"PFD"
				],
				"Brakes -- RELEASE"
			]
		],
		[
			"BEFORE TAKEOFF",
			[
				"Elevator Trim -- SET 3-6° NOSE UP",
				"Flaps -- TO",
				"Pitot Heat -- AS REQUIRED",
				"Lights -- AS REQUIRED"
			],
			"RUNUP",
			[
				"Parking Brake -- SET",
				"Engine Instruments -- CHECK WITHIN OPER. LIMITS",
				"Flight Instruments -- CHECK",
				"Throttle -- 1700 RPM",
				"Propeller -- EXERCISE",
				[
					{
						"ul": true
					},
					"(to obtain 200 to 300 RPM drop)"
				],
				"Magnetos -- CHECK INDIVIDUALLY",
				"Alternator 2 and Bus Tie -- CHECK",
				[
					"Throttle -- ≥ 2000 RPM",
					[
						{
							"ul": true
						},
						"[BUSES TIED] -- Extinguished",
						"Voltmeter 2: 27.5 - 29.0 VOLTS",
						"Loadmeter 2: POSITIVE LOAD"
					],
					"Throttle -- 1000 - 1200 RPM",
					[
						{
							"ul": true
						},
						"[BUSES TIED] - Illuminated"
					]
				],
				"Flight Controls -- CHECK FREEDOM OF MOVEMENT AND PROPER DIRECTION OF TRAVEL",
				"Doors and Windows -- SECURE",
				"Fuel Selector Valve -- CHECK TANK SELECTED",
				"Aux Fuel Pump -- OFF",
				"Alerts/Messages -- EXTINGUISHED OR CONSIDERED",
				"PFD Attitude and Heading -- NORMAL",
				"Parking Brake -- RELEASE"
			]
		],
		[
			"TAKEOFF",
			[
				"Take-Off Power -- SET",
				[
					"Throttle -- FULL FORWARD",
					"Propeller -- HIGH RPM",
					"Mixture -- SET FUEL FLOW AT CYAN CLIMB FUEL FLOW MARKER",
					"[BUSES TIED] -- EXTINGUISHED",
					"Brakes -- RELEASE",
					"Instruments -- CHECK"
				]
			]
		],
		[
			"CLIMB",
			[
				"Landing Gear -- RETRACT",
				"Flaps -- RETRACT",
				"Power -- SET",
				[
					"Throttle -- FULL FORWARD",
					"Propeller -- 2500 or 2700 RPM",
					"Mixture -- MAINTAIN FUEL FLOW AT CYAN CLIMB FUEL FLOW MARKER"
				],
				"Cowl Flaps -- AS REQUIRED",
				"Airspeed -- 100 - 110 KTS",
				"Aux Fuel Pump -- AS REQUIRED",
				"Lights -- AS REQUIRED"
			]
		],
		[
			"CRUISE",
			[
				"Cowl Flaps -- CLOSE",
				"Power -- SET",
				"Aux Fuel Pump -- OFF",
				"Mixture -- LEAN USING EGT",
				"Lights -- AS REQUIRED",
				"Fuel Selector Valve -- SELECT FULLER TANK"
			]
		],
		[
			"APPROACH",
			[
				"Altimeters -- SET",
				"Lights -- AS REQUIRED",
				"Propeller -- HIGH RPM",
				"Mixture -- FULL RICH",
				"Landing Gear (154 kts or below) -- DOWN AND CHECK"
			]
		],
		[
			"LANDING",
			[
				"Flaps (124 kts or below) -- DOWN",
				"Yaw Damp -- OFF"
			]
		],
		[
			"AFTER LANDING",
			[
				"Cowl Flaps -- OPEN",
				"Flaps -- UP",
				"Lights -- AS REQUIRED",
				"Trim Tabs -- RESET AS REQUIRED",
				"[BUSES TIED] -- ILLUMINATED"
			]
		],
		[
			"SHUTDOWN AND SECURING",
			[
				"Parking Brake -- SET",
				"Avionics Master -- OFF",
				"Throttle -- 1000 RPM",
				"Mixture -- CUT OFF",
				"Magnetos -- OFF",
				"Battery 1 & 2, Alternator 1 & 2 -- OFF"
			]
		]
	]
}